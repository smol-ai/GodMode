{
	"name": "smolmenubar",
	"productName": "smolmenubar",
	"version": "0.0.15",
	"author": "smol-ai",
	"description": "smol menubar app for ChatGPT, Claude, Bing, Bard",
	"main": "index.js",
	"scripts": {
		"launch": "NODE_ENV=development electron-forge start",
		"start": "NODE_ENV=development node setup.js && NODE_ENV=development electron-forge start",
		"package": "electron-forge package --arch arm64,x64",
		"make": "electron-forge make --arch arm64,x64",
		"build": "npm run make",
		"buildAndSign": "NODE_ENV=sign npm run make",
		"publish": "electron-forge publish --arch arm64,x64",
		"format": "prettier '**/*.js' --write",
		"lint": "echo \"No linting configured\""
	},
	"config": {
		"forge": "./forge.config.js"
	},
	"keywords": [
		"chatgpt",
		"openai",
		"mac"
	],
	"license": "MIT",
	"dependencies": {
		"electron-context-menu": "^3.6.0",
		"electron-log": "^5.0.0-beta.24",
		"electron-store": "^8.1.0",
		"menubar": "^9.2.3",
		"update-electron-app": "^2.0.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/smol-ai/menubar"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.1.1",
		"@electron-forge/maker-deb": "^6.1.1",
		"@electron-forge/maker-dmg": "^6.0.4",
		"@electron-forge/maker-rpm": "^6.1.1",
		"@electron-forge/maker-squirrel": "^6.1.1",
		"@electron-forge/maker-zip": "^6.1.1",
		"@electron-forge/publisher-github": "^6.0.4",
		"child_process": "^1.0.2",
		"dotenv": "^16.0.3",
		"electron": "^22.3.12",
		"electron-squirrel-startup": "^1.0.0",
		"fs": "^0.0.1-security",
		"prettier": "^3.0.0",
		"windows-shortcuts": "^0.1.6"
	}
}
